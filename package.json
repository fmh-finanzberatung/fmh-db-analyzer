{
  "name": "fmh-db-analyzer",
  "version": "0.0.1",
  "description": "A node based database analyzer",
  "main": "lib/db-analyzer.js",
  "repository": "https://github.com/fmh-finanzberatung/fmh-db-analyzser.git",
  "author": "John Galt",
  "license": "MIT",
  "scripts": {
    "db:mongo:seed": "NODE_ENV=test node test/mongo/seed/runner.js",
    "db:mysql:create": "NODE_ENV=test node test/mysql/db/create.js",
    "db:mysql:drop": "NODE_ENV=test node test/mysql/db/drop.js",
    "db:mysql:migrate": "NODE_ENV=test knex migrate:latest --knexfile=knexfile.js",
    "db:mysql:migration": "NODE_ENV=test npx knex migrate:make --knexfile=knexfile.js",
    "db:mysql:reset": "yarn db:mysql:drop && yarn db:mysql:create",
    "db:mysql:rollback": "NODE_ENV=test knex migrate:rollback --knexfile=knexfile.js",
    "db:mysql:seed": "NODE_ENV=test node test/mysql/seed/runner.js",
    "dev:mysql": "NODE_ENV=development node dev/mysql-schema-server.js",
    "dev:mongo": "NODE_ENV=development node dev/mongo-schema-server.js",
    "dev:example:code": "NODE_ENV=development node dev/server-example-code.js",
    "dev:example:sdl": "NODE_ENV=development node dev/server-example-sdl.js",
    "test": "yarn test:graph-node && yarn test:graphql-builder && yarn test:model-builder && yarn test: traverse-nodes",
    "test:graph-node": "NODE_ENV=test node test/graph-node.test.js",
    "test:graphql-mongo-schema-adapters": "NODE_ENV=test node test/graphql-mongo-schema-adapters.test.js",
    "test:graphql-mysql-schema-adapters": "NODE_ENV=test node test/graphql-mysql-schema-adapters.test.js",
    "test:graphql-mysql-schema-builder": "NODE_ENV=test node test/graphql-mysql-schema-builder.test.js | tap-spec",
    "test:graphql-type-builder": "NODE_ENV=test node test/graphql-type-builder.test.js | tap-spec",
    "test:graphql-schema-builder-example.test.js": "NODE_ENV=test node test/graphql-schema-builder-example.test.js",
    "test:id-obfuscator": "NODE_ENV=test node test/id-obfuscator.test.js",
    "test:mongo-index-reader": "NODE_ENV=test node test/mysql/mysql-index-reader.test.js",
    "test:mongo-model-builder": "NODE_ENV=test node test/mongo/mongo-model-builder.test.js",
    "test:mongo-schema-adapters": "NODE_ENV=test node test/mongo/mongo-schema-adapters.test.js",
    "test:mongo-schema-reader": "NODE_ENV=test node test/mongo/mongo-schema-reader.test.js",
    "test:mysql-index-reader": "NODE_ENV=test node test/mysql/mysql-index-reader.test.js",
    "test:mysql-model-builder": "NODE_ENV=test node test/mysql/mysql-model-builder.test.js",
    "test:mysql-schema-adapters": "NODE_ENV=test node test/mysql/mysql-schema-adapters.test.js",
    "test:mysql-schema-reader": "NODE_ENV=test node test/mysql/mysql-schema-reader.test.js",
    "test:node-edge-inspector": "NODE_ENV=test node test/node-edge-inspector.test.js",
    "test:traverse-nodes": "NODE_ENV=test node test/traverse-nodes.test.js | tap-spec"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "bookshelf": "^1.2.0",
    "bookshelf-cascade-delete": "^2.0.1",
    "bookshelf-scopes": "^1.5.1",
    "cli-highlight": "^2.1.11",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fast-glob": "^3.2.5",
    "graphql-schema": "^0.5.1",
    "humanize-duration": "^3.26.0",
    "knex": "0.21.17",
    "mk-env-vars": "https://github.com/mkastner/mk-env-vars.git#v0.0.7",
    "mongodb": "^3.6.6",
    "mysql": "^2.18.1",
    "pluralize": "^8.0.0",
    "prettier": "^2.2.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.2.2"
  },
  "dependencies": {
    "dedent-js": "^1.0.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "mk-log": "https://github.com/mkastner/mk-log.git#v0.0.12"
  }
}
