{
  "name": "fmh-db-analyzer",
  "version": "0.0.17",
  "description": "A node based GraphQL schema generator",
  "main": "lib/db-analyzer.js",
  "repository": "https://github.com/fmh-finanzberatung/fmh-db-analyzser.git",
  "author": "John Galt",
  "license": "MIT",
  "scripts": {
    "cli": "NODE_ENV=test node scripts/cli.js",
    "front:dev": "vite --port=3010 --host",
    "front:build": "vite build",
    "front:serve": "vite preview",
    "db:mysql:create": "NODE_ENV=test node test/mysql/db/create.js",
    "db:mysql:drop": "NODE_ENV=test node test/mysql/db/drop.js",
    "db:mysql:migrate": "NODE_ENV=test knex migrate:latest --knexfile=knexfile.js",
    "db:mysql:migration": "NODE_ENV=test npx knex migrate:make --knexfile=knexfile.js",
    "db:mysql:reset": "yarn db:mysql:drop && yarn db:mysql:create && yarn db:mysql:migrate && yarn db:mysql:seed",
    "db:mysql:rollback": "NODE_ENV=test knex migrate:rollback --knexfile=knexfile.js",
    "db:mysql:seed": "NODE_ENV=test node test/mysql/seed/runner.js",
    "dev:example:code": "NODE_ENV=development node dev/server-example-code.js",
    "dev:example:sdl": "NODE_ENV=development node dev/server-example-sdl.js",
    "dev:mysql": "NODE_ENV=development node dev/mysql-schema-server.js",
    "dev:code:list:mysql": "madge --circular --orphans --warning ./dev/mysql-schema-server.js",
    "dev:code:graph:mysql": "madge --circular --image tmp/mysql.svg ./dev/mysql-schema-server.js",
    "test": "yarn test:graph-node && yarn test:graphql-builder && yarn test:model-builder && yarn test: traverse-nodes",
    "test:config-reader": "NODE_ENV=test node test/config-reader.test.js",
    "test:graph-node": "NODE_ENV=test node test/graph-node.test.js",
    "test:code-first": "NODE_ENV=test node test/graphql/code-first.test.js",
    "test:circular-dependency": "NODE_ENV=test node test/graphql/circular-dependency.test.js",
    "test:graphql-type-builder": "NODE_ENV=test node test/graphql/type-builder.test.js",
    "test:graphql-node-builder": "NODE_ENV=test node test/graphql/node-builder.test.js",
    "test:graphql-mysql-resolve-builder": "NODE_ENV=test node test/resolvers/graphql-mysql-resolve-builder.test.js",
    "works:test:mysql-schema-journal-adapters": "",
    "test:mysql-schema-journal-adapters": "NODE_ENV=test node test/mysql-schema-journal-adapters.test.js",
    "test:graphql-mysql-schema-builder": "NODE_ENV=test node --trace-warnings test/graphql-mysql-schema-builder.test.js | tap-spec",
    "test:graphql-mysql-auth-plugin": "NODE_ENV=test node --trace-warnings test/plugins/graphql-mysql-auth.plugin.test.js | tap-spec",
    "test:graphql-mysql-admin-plugin": "NODE_ENV=test node --trace-warnings test/plugins/graphql-mysql-admin.plugin.test.js | tap-spec",
    "test:graphql-mysql-type-builder": "NODE_ENV=test node test/graphql-mysql-type-builder.test.js | tap-spec",
    "test:graphql-schema-builder-example": "NODE_ENV=test node test/graphql-schema-builder-example.test.js",
    "test:id-obfuscator": "NODE_ENV=test node test/id-obfuscator.test.js",
    "test:mailers:registration": "NODE_ENV=test node test/mailers/registration-mailer.test.js | tap-spec",
    "test:mysql-index-reader": "NODE_ENV=test node test/mysql/mysql-index-reader.test.js",
    "test:mysql-model-builder": "NODE_ENV=test node test/mysql/mysql-model-builder.test.js",
    "test:mysql-schema-reader": "NODE_ENV=test node test/mysql/mysql-schema-reader.test.js",
    "test:node-edge-inspector": "NODE_ENV=test node test/node-edge-inspector.test.js",
    "test:traverse-nodes": "NODE_ENV=test node test/traverse-nodes.test.js | tap-spec",
    "test:utils:cal-c": "NODE_ENV=test node test/utils/cal-c.test.js | tap-spec",
    "test:utils:js-data-type": "NODE_ENV=test node test/js-data-type.test.js",
    "test:utils:plugin-manager": "NODE_ENV=test node test/utils/plugin-manager.test.js",
    "test:utils:random-string": "NODE_ENV=test node test/utils/random-string.test.js",
    "test:utils:wrap-text": "NODE_ENV=test node test/utils/wrap-text.test.js | tap-spec"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@eslint/create-config": "^0.1.2",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
    "axios": "^0.24.0",
    "babel-eslint-parser": "^7.13.10",
    "cli-highlight": "^2.1.11",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "eslint-plugin-vue": "^8.4.1",
    "humanize-duration": "^3.26.0",
    "imapflow": "^1.0.86",
    "mailparser": "^3.4.0",
    "prettier": "^3.0.1",
    "prettier-plugin-svelte": "^2.6.0",
    "sass": "^1.43.5",
    "svelte": "^3.37.0",
    "svelte-preprocess": "^4.9.8",
    "svelte-routing": "^1.6.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.2.2",
    "vite": "^2.6.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "bookshelf": "^1.2.0",
    "bookshelf-cascade-delete": "^2.0.1",
    "bookshelf-scopes": "^1.5.1",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dedent-js": "^1.0.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "fast-glob": "^3.2.5",
    "global": "^4.4.0",
    "graphql": "^16.3.0",
    "graphql-schema": "^0.5.1",
    "graphql-tools": "^8.2.0",
    "handlebars": "^4.7.7",
    "http-terminator": "^3.0.3",
    "jose": "^4.5.0",
    "knex": "0.21.17",
    "mk-env-vars": "https://github.com/mkastner/mk-env-vars.git#v0.0.9",
    "mk-log": "https://github.com/mkastner/mk-log.git#v0.0.13",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.2",
    "pluralize": "^8.0.0"
  }
}
